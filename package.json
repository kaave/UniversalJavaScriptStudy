{
  "name": "UniversalReact",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all -p start:client start:server",
    "start:client": "NODE_ENV=development webpack --config=webpack/client/index.js --progress --color -w",
    "start:server": "NODE_ENV=development npm run build:server && node ./dist/server.js",
    "build": "NODE_ENV=production rimraf dist/ && npm-run-all -p build:client build:server",
    "build:client": "webpack --config=webpack/client/index.js --progress --color",
    "build:server": "webpack --config=webpack/server/index.js --progress --color",
    "modulecheck": "rimraf dist/ && rimraf node_modules && npm i && npm start"
  },
  "dependencies": {
    "date-fns": "^1.28.2",
    "ejs": "^2.5.6",
    "express": "^4.14.1",
    "fetchr": "^0.5.36",
    "normalize.css": "^6.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.1",
    "redux-logger": "^3.0.1",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-preset-env": "^1.3.2",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "isomorphic-style-loader": "^1.1.0",
    "npm-run-all": "^4.0.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "uglifyjs-webpack-plugin": "^0.4.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.0"
  }
}
